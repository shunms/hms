#include "golf_info.h"


Golf_Info::Golf_Info( ) 
{
}

Golf_Info::~Golf_Info( ) 
{
}


void Golf_Info::SetNumGolfCarts( int number )
{
    numGolfCarts = number;
}

void Golf_Info::SetNumRHandClubs( int number )
{
    numRHandClubs = number;
}

void Golf_Info::SetNumLHandClubs( int number )
{
    numLHandClubs = number;
}


int Golf_Info::GetNumGolfCarts( )
{
    return numGolfCarts;
}

int Golf_Info::GetNumRHandClubs( )
{
    return numRHandClubs;
}

int Golf_Info::GetNumLHandClubs( )
{
    return numLHandClubs;
}
















