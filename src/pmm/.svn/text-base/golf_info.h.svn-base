#ifndef GOLF_INFO_H
#define GOLF_INFO_H


#include <QObject>


/**
* The Golf_Info class provides the foundation for the
* data attributes of the Golf_Info class.
*/

class Golf_Info //: public QObject
{
    //Q_OBJECT

    public:

        /**
        * Constructor for Restaurant_Info class.
        */
        Golf_Info( );

		/**
        * Destructor for Restaurant_Info class.
        */
        ~Golf_Info( );


        /**
        * Sets the number of Golf carts.
        */
        void SetNumGolfCarts( int number );

		/**
        * Sets the number of right hand clubs.
        */
        void SetNumRHandClubs( int number );

        /**
        * Sets the number of left hand clubs.
        */
        void SetNumLHandClubs( int number );

        /**
        * Gets the number of Golf carts.
        */
        int GetNumGolfCarts( );

		/**
        * Gets the number of right hand clubs.
        */
        int GetNumRHandClubs( );

        /**
        * Gets the number of left hand clubs.
        */
        int GetNumLHandClubs( );



    private:
		int		numGolfCarts;
		int		numRHandClubs;
		int		numLHandClubs;
 
};



#endif

// EOF

